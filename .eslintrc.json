{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base","plugin:jest/recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "semi": ["error", "never"],
    "prefer-const": "off",
    "import/no-muttable-exports": "off",
    "import/named": "off",
    "no-throw-literal": "off"
  }
}
